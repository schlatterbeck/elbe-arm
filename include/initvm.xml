<initvm>
  <buildtype>amd64</buildtype>
  <mirror>
    <primary_host>bee.priv.zoo</primary_host>
    <primary_path>/debian</primary_path>
    <primary_proto>http</primary_proto>
    <url-list>
      <url>
        <binary>
          http://debian.linutronix.de/elbe-testing stretch main
        </binary>
      </url>
      <url>
        <binary>
          http://debian.linutronix.de/elbe-common stretch main
        </binary>
      </url>
    </url-list>
  </mirror>
  <suite>stretch</suite>
  <pkg-list>
    <pkg>openssh-server</pkg>
  </pkg-list>
  <portforwarding>
    <forward>
      <proto>tcp</proto>
      <buildenv>22</buildenv>
      <host>5022</host>
    </forward>
    <forward>
      <proto>tcp</proto>
      <buildenv>7588</buildenv>
      <host>7587</host>
    </forward>
  </portforwarding>
</initvm>
